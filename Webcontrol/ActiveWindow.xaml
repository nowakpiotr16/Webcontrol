<Window x:Class="Webcontrol.ActiveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Webcontrol"
        mc:Ignorable="d"
        Title="Web Control" Height="75" Width="235.763"
        Topmost="true">
    <Grid>
        <Button x:Name="nextButton" Content="Next" HorizontalAlignment="Left" Margin="25,10,0,0" VerticalAlignment="Top" Width="75" Click="nextButton_Click" />
        <Button x:Name="randomButton" Content="Random" HorizontalAlignment="Left" Margin="125,10,0,0" VerticalAlignment="Top" Width="75" Click="randomButton_Click" />

    </Grid>
</Window>
